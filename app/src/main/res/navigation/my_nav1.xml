<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav1"
    app:startDestination="@id/listFragment">

    <fragment
        android:id="@+id/listFragment"
        android:name="com.example.todolistapp.onboarding.screen.ListTask"
        android:label="fragment_list"
        tools:layout="@layout/fragment_list_task" >
        <action
            android:id="@+id/action_listFragment_to_addFragment2"
            app:destination="@id/addFragment" />
    </fragment>
    <fragment
        android:id="@+id/addFragment"
        android:name="com.example.todolistapp.onboarding.screen.BottomSheet"
        android:label="fragment_add"
        tools:layout="@layout/fragment_bottom_sheet" >
        <action
            android:id="@+id/action_bottomSheetFragment_to_fragmentListTask"
            app:destination="@id/listFragment" />
    </fragment>
</navigation>